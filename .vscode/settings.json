{
  "files.exclude": {
    "**/bin": true,
    "**/[Dd]ebug/": true,
    "**/[Rr]elease/": true,
    "**/x64/": true,
    "**/[Bb]in/": true,
    "**/[Oo]bj/": true,
    "**/*.swp": true,
    "**/*.*~": true,
    "**/project.lock.json": true,
    "**/.DS_Store": true,
    "**/*.pyc": true,
    "**/nupkg/": true,
    "**/.vscode": true,
    "**/.idea": true,
    "**/*.suo": true,
    "**/*.user": true,
    "**/*.userosscache": true,
    "**/*.sln.docstates": true,
    "**/[Dd]ebugPublic/": true,
    "**/[Rr]eleases/": true,
    "**/x86/": true,
    "**/build/": true,
    "**/bld/": true,
    "**/[Oo]ut/": true,
    "**/msbuild.log": true,
    "**/msbuild.err": true,
    "**/msbuild.wrn": true,
